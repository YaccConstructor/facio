              <li><a href="http://fsprojects.github.io/FsLexYacc/">FsLexYacc</a></li>
              <li><a href="http://fsharp.org">fsharp.org</a></li>
              <li><a href="@Properties["project-github"]">github page</a></li>
          </ul>
        <h3 class="muted"><a href="@Root/index.html">@Properties["project-name"]</a></h3>
      </div>
      <hr />
      <div class="row">
        <div class="span9" id="main">
          @RenderBody()
        </div>
        <div class="span3">
          <img src="@Root/img/logo.png" alt="F# Project" style="width:150px;margin:10px;padding-right: 115px;" />
          <a href="https://travis-ci.org/jack-pappas/facio" style="margin-left: 20px;">
            <img src="https://travis-ci.org/jack-pappas/facio.png" alt="travis-ci" />
          </a>
          <ul class="nav nav-list" id="menu" style="margin-top: 0px;">
            <li class="nav-header">@Properties["project-name"]</li>
            <li><a href="@Root/index.html">Home page</a></li>
